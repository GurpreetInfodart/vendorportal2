@model IEnumerable<VendorPortal.Models.SchedulerViewModel>

@{
    ViewBag.Title = "SechedulerBuyerDetail";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<link href="~/Content/css/jquery.dataTables.css" rel="stylesheet" />

<div class="form-horizontal">
    <div class="row">
        <div class="col-lg-12">
            <!--breadcrumbs start -->
            <ul class="breadcrumb">
                <li><a href="#"><i class="fa fa-home"></i>Schedule Management</a></li>
                <li><a href="#">Buyer Scheduler</a></li>
                <li class="active">ACKNOWLEDGE/DECLINE Scheduler</li>
            </ul>
            <!--breadcrumbs end -->
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12">

            <section class="panel">
                <!-- <header class="panel-heading">
                   (*) Mandatory fields to be filled in by Vendor / Requestor
                </header>-->
                <div class="panel-body">
                    <form class="form-horizontal" role="form">


                        <div class="row">
                            @*<div class="table-responsive panel-body">
                                    <table align="left">
                                        <tr>
                                            <th>
                                                From Date: <input type="date" id="Fromdate">
                                                To Date:<input type="date" id="Todate">
                                                <input type="button" id="MyButton" value="Find" onclick="date()" />

                                            </th>
                                        </tr>
                                    </table>
                                </div>*@
                            <div class="table-responsive">
                                <table class="table table-bordered table-condensed table-hover table-striped" id="pending" align="left">
                                    <thead>
                                        <tr>
                                            <th>Sr. No.</th>
                                            <th>
                                                Plant Name
                                            </th>
                                            <th>
                                                Supplier Code
                                            </th>
                                            <th>
                                                Supplier Name
                                            </th>
                                            <th>
                                                Item Code
                                            </th>
                                            <th>
                                                Item Description
                                            </th>
                                            <th>
                                                UOM
                                            </th>
                                            <th>
                                                Position No.
                                            </th>
                                            <th>
                                                Schedule No.
                                            </th>
                                            <th>
                                                Quantity
                                            </th>
                                            <th>
                                                Delv. Date  
                                            </th>
                                            <th>
                                                Remark
                                            </th>
                                             
                                        </tr>
                                    </thead>
                                    <tbody>

                                        @{ int rowNo = 1;}
                                        @foreach (var item in Model)
                                        {
                                            <tr>
                                                <td style="color:black;font-size:small">
                                                    @rowNo
                                                </td>
                                                <td style="color:black;font-size:small">
                                                    @item.Plant_Name
                                                </td>
                                                <td style="color:black;font-size:small">
                                                    @item.SUPP_CODE
                                                </td>
                                                <td style="color:black;font-size:small">
                                                    @item.SUPP_NAME
                                                </td>
                                                <td style="color:black;font-size:small">
                                                    @item.MaterialCode
                                                </td>    
                                                <td style="color:black;font-size:small">
                                                    @item.MaterialDescription
                                                </td>    
                                                <td style="color:black;font-size:small">
                                                    @item.UOM
                                                </td>    
                                                <td style="color:black;font-size:small">
                                                    @item.Position
                                                </td>    
                                                <td style="color:black;font-size:small">
                                                    @item.Schedule_NO
                                                </td>   
                                                <td style="color:black;font-size:small">
                                                    @item.Qty
                                                </td>    
                                                <td style="color:black;font-size:small">
                                                    @item.Delv_Date
                                                </td>   
                                                <td style="color:black;font-size:small">
                                                    @item.Remark
                                                </td>    
                                            </tr>
                                            rowNo++;
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="col-lg-12">
                            <div class="row">
                                <a href="@Url.Action("ViewBuyerSchedule")">Back To List</a>
                            </div>
                        </div>
                    </form>

                </div>
            </section>
        </div>


    </div>



</div>





 

